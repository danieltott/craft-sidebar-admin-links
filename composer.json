    "docs": "https://github.com/danieltott/craft-sidebar-admin-links/blob/master/README.md",
    "issues": "https://github.com/danieltott/craft-sidebar-admin-links/issues"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Dan Ott",
      "homepage": "https://dtott.com"
    }
